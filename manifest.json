{
    "manifest_version":2,
    "name": "TransTan",
    "version": "1.0",
    "description": "A fourth extension by Tan",
    "icons":{
        "128":"icon128.png",
        "48":"icon128.png",
        "16":"icon128.png"
    },
    "browser_action":{
        "default_icon":"icon16.png",
        "default_popup":"popup.html"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },
    "permissions": ["tabs","contextMenus"]
}